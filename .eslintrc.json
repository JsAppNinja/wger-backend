{
  "extends": "airbnb/legacy",
  "env": {
    "browser": true,
    "jquery": true
  },
  "globals": {
    "d3": true
  },
  "rules": {
    "func-names": [
      "error",
      "never"
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "varsIgnorePattern": "^wger"
      }
    ]
  }
}
